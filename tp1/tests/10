let 

	type tipo1 = { id: int, nome:string, idade: int }
	type tipo2 = array of string

	var a := tipo2 [10] of 0
	var b  := tipo2 [5] of tipo1 {id=4, nome="fran", idade=50}
	var c : tipo2 := tipo2 [100] of ""
	var d := tipo1 {id=1, nome="zé", idade=20 }
	var e := tipo2 {id=2, nome="jão", idade = 30}

in
	d[2].nome := "maria";
	d[0].age := 40;
    a[5] := 114; 
    b[4] := 6;
    c[8] := "aladim";
end
